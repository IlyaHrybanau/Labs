#include <iostream>
using namespace std;

int main() {
    int number;
    cin >> number;

    int tens = number / 10;
    int units = number % 10;

    int square = number * number;
    int sumOfCubes = 4 * (tens * tens * tens + units * units * units);

    if (square == sumOfCubes) {
        cout << "Yes" << endl;
    } else {
        cout << "No" << endl;
    }

    return 0;
}
