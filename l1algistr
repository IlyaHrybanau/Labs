#include <iostream>
#include <cstdlib>
#include <ctime>
#include <cmath>

using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");

    int n = 5000;

    int** matrix = new int*[n];
    for (int i = 0; i < n; ++i) {
        matrix[i] = new int[n];
    }

    srand(time(0));

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            matrix[i][j] = rand() % 10 + 1;
        }
    }

    double log_proizv = 0.0;
    for (int i = 0; i < n; ++i) {
        log_proizv += log(matrix[i][i]);
    }

    cout << "Сумма натуральных логарифмов диагональных элементов: " << log_proizv << endl;

    for (int i = 0; i < n; ++i) {
        delete[] matrix[i];
    }
    delete[] matrix;

    return 0;
}
