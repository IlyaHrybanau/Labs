//Ex.2
#include <iostream>
#include <cstdlib>  
#include <ctime>    
#include <iomanip>  

int main() {
  setlocale(LC_ALL, ".1251");
  const int n = 10;
  const int m = 5;
  int i, j;
  int arr[n][m];

  for (i = 0; i < n; i++) {
    for (j = 0; j < m; j++) {
      arr[i][j] = rand() % 100;
    }  
  }
    

  for (i = 0; i < n; i++) {
    for (j = 0; j < m; j++) {
      cout << "\t" << arr[i][j];
    }
      
  }
    
  
  return 0;
}